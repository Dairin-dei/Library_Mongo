{
  "name": "CRUD-API",
  "version": "1.0",
  "private": true,
  "start": "nodemon ./src/index.ts",
  "scripts": {
    "test": "jest --silent --forceExit --runInBand",
    "test-coverage": "jest --coverage --silent --forceExit  --runInBand",
    "start:dev": "nodemon ./src/index.ts",
    "start:prod": "npm run build && npm run prod",
    "prod": "node dist/main.js",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.2",
    "@types/node": "^17.0.42",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongodb": "^4.7.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/.multi.ts"
    ],
    "transform": {},
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
